(executables
  (names bswap)
  (libraries)
  (modes js)
)

(rule
  (target bswap.referencejs)
  (deps bswap.bc.js)
  (action (with-stdout-to %{target} (run node ./bswap.bc.js))))

(alias
  (name runtest)
  (deps bswap.reference bswap.referencejs)
  (action (diff bswap.reference bswap.referencejs)))
